{
  "name": "@fiu-fit/workout-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "db:dev:rm": "docker compose rm dev-db -s -f -v",
    "db:dev:up": "docker compose up dev-db -d",
    "db:dev:restart": "yarn db:dev:rm && yarn db:dev:up && (sleep 1 || timeout 1)",
    "db:test:rm": "docker compose rm test-db -s -f -v",
    "db:test:up": "docker compose up test-db -d",
    "db:test:restart": "yarn db:test:rm && yarn db:test:up && (sleep 1 || timeout 1) && yarn prisma:test:deploy",
    "purge": "rimraf node_modules",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "yarn db:dev:up && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "dotenv -e .env.test -- jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "yarn db:test:restart",
    "test:e2e": "dotenv -e .env.test -- jest --no-cache --config ./test/jest-e2e.json",
    "postinstall": "yarn husky install",
    "lint:prettier": "prettier --config .prettierrc.json --ignore-path .prettierignore --write",
    "lint:eslint": "eslint --fix -c .eslintrc.json --ignore-path .eslintignore",
    "lint": "yarn run lint:prettier && yarn run lint:eslint",
    "common:install": "yarn add ssh://git@github.com:Fiu-Fit/common.git && yarn",
    "proto:user": "npx protoc --plugin=node_modules/.bin/protoc-gen-ts_proto -I=./node_modules/@fiu-fit/common/src/protos --ts_proto_out=src/modules/user/interfaces/ node_modules/@fiu-fit/common/src/protos/user.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto:auth": "npx protoc --plugin=node_modules/.bin/protoc-gen-ts_proto -I=./node_modules/@fiu-fit/common/src/protos --ts_proto_out=src/modules/auth/interfaces/ node_modules/@fiu-fit/common/src/protos/auth.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto:all": "yarn run common:install && yarn run proto:user && yarn run proto:auth",
    "proto:user:win": "npx protoc --plugin=node_modules/.bin/protoc-gen-ts_proto.cmd -I=./node_modules/@fiu-fit/common/src/protos --ts_proto_out=src/modules/user/interfaces/ node_modules/@fiu-fit/common/src/protos/user.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto:auth:win": "npx protoc --plugin=node_modules/.bin/protoc-gen-ts_proto.cmd -I=./node_modules/@fiu-fit/common/src/protos --ts_proto_out=src/modules/auth/interfaces/ node_modules/@fiu-fit/common/src/protos/auth.proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb",
    "proto:all:win": "yarn run common:install && yarn run proto:user:win && yarn run proto:auth:win"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seeder.ts"
  },
  "dependencies": {
    "@fiu-fit/common": "ssh://git@github.com:Fiu-Fit/common.git",
    "@grpc/grpc-js": "^1.8.13",
    "@grpc/proto-loader": "^0.7.6",
    "@nestjs/common": "^9.3.12",
    "@nestjs/core": "^9.3.12",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/microservices": "^9.3.12",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.12",
    "@prisma/client": "^4.12.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^4.18.2",
    "husky": "^7.0.2",
    "mongoose": "^7.0.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.1",
    "rxjs": "^7.8.0",
    "ts-proto": "^1.146.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.3.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.12",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "dotenv-cli": "^7.1.0",
    "esbuild": "^0.17.14",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^10.5.0",
    "prettier": "^2.8.7",
    "prisma": "^4.12.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  }
}
